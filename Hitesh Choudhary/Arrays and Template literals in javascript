<body>
<script>
let myName = ['Yasin','Arif','Sohan']
	 	document.write(myName[0]);
		document.write('<br>');
		document.write(myName[myName.length-1]);
		document.write('<br>');
		document.write(`I have ${myName.length} Name`);// back semeclon tab ar upporar button
		document.write('<br>');
	document.write(`I have ${10*20} Tk`);
</script>
