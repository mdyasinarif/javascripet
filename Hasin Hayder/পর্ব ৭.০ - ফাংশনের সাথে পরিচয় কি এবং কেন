<script type="text/javascript">
function myfunction(name){
  if(name){
    document.write("Hello"+name+"<br />");
}else{
  document.write("How are you"+"<br />");
}
}
myfunction("Yasin");
myfunction("");

function isEven(number){
  if(number%2==0){
    return true;
  }
    return false;
}
document.write(isEven(20)+"<br />")
document.write(isEven(7)+"<br />")

</script>
