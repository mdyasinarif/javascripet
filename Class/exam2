  <title>Document</title>
</head>
<body>
  <script type="text/javascript">
  var sum = 0;
  var sn = prompt("1st number","");
  var en = prompt("2nd number","");
  function makeSum(sn,en){
    while(sn>=en){
      sum+=sn;
      sn--;
    }
    return sum;
  }
  alert(makeSum(sn,en));
  </script>

</body>
</html>
